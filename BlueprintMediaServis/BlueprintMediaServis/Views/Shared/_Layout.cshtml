<!DOCTYPE html>
<html>
<head id="Head1" runat="server">
    
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <meta charset="utf-8" />
    <title>AE Arma-Elektropanç</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, shrink-to-fit=no" />
    
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-touch-fullscreen" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="default">
    <meta content="" name="description" />
    <meta content="" name="author" />
    <link href="assets/plugins/pace/pace-theme-flash.css" runat = "server" rel="stylesheet" type="text/css" />
    <link href="assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="assets/plugins/font-awesome/css/font-awesome.css" rel="stylesheet" type="text/css" />
    <link href="assets/plugins/jquery-scrollbar/jquery.scrollbar.css" rel="stylesheet" type="text/css" media="screen" />
    <link href="assets/plugins/select2/css/select2.min.css" rel="stylesheet" type="text/css" media="screen" />
    <link href="assets/plugins/switchery/css/switchery.min.css" rel="stylesheet" type="text/css" media="screen" />
    <link href="pages/css/pages-icons.css" rel="stylesheet" type="text/css">
    <!-- include summernote css/js -->
    <link href="dist/summernote.css" rel="stylesheet" type="text/css">
    
    <link rel="icon" type="image/x-icon" href="~/assets/img/favicon.ico" />
    <link class="main-stylesheet" href="pages/css/pages.css" rel="stylesheet" type="text/css" />

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")

</head>

<body class="fixed-header menu-pin ">
    <!-- BEGIN SIDEBPANEL-->
    <nav class="page-sidebar" data-pages="sidebar">
        <div class="sidebar-header">
            <img src="assets/img/logo2.png" alt="logo" class="brand" data-src="assets/img/logo.png" data-src-retina="assets/img/logo.png" width="70" height="70">
            <div class="sidebar-header-controls ">
                
                <!--<button type="button" class="btn btn-link hidden-md-down " data-toggle-pin="sidebar">
                    <i class="fa fs-12 pull-right"></i>
                </button>-->
            </div>
        </div>
        <div class="sidebar-menu">
            <!-- BEGIN SIDEBAR MENU ITEMS-->
            <ul class="menu-items">
                <li class="">
                    @Html.ActionLink("Anasayfa", "Index", "Home")
                    <span class="bg-succes icon-thumbnail"><i class="pg-home"></i></span>
                </li>
                <li class="">
                    <a href="javascript:;">
                        <span class="title">Dergiler</span>
                        <span class="arrow"></span>
                    </a>
                    <span class="icon-thumbnail"><i class="pg-menu_lv"></i></span>
                    <ul class="sub-menu">
                        <li>
                            @Html.ActionLink("Kategori Ekle", "Index", "MagazineCategory")
                            <span class="icon-thumbnail"><i class="pg-plus"></i></span>
                        </li>
                        <li>
                            @Html.ActionLink("Listele", "Index", "Magazine")
                            <span class="icon-thumbnail"><i class="pg-menu"></i></span>
                        </li>
                    </ul>
                </li>
                <li class="">
                    @Html.ActionLink("Katalog", "Index", "Catalog")
                    <span class="icon-thumbnail"><i class="pg-note"></i></span>
                </li>
                <li class="">
                    @Html.ActionLink("Video", "Index", "Video")
                    <span class="icon-thumbnail"><i class="pg-video"></i></span>
                </li>
                <li class="">
                    <a href="javascript:;">
                        <span class="title">Sayfalar</span>
                        <span class="arrow"></span>
                    </a>
                    <span class="icon-thumbnail"><i class="pg-menu_lv"></i></span>
                    <ul class="sub-menu">
                        <li>
                            @Html.ActionLink("Sayfa Ekle", "Index", "Menu")
                            <span class="icon-thumbnail"><i class="pg-plus"></i></span>
                        </li>
                        <li>
                            @Html.ActionLink("Listele", "Index", "PagesList")
                            <span class="icon-thumbnail"><i class="pg-menu"></i></span>
                        </li>
                    </ul>
                </li>

                <li class="">
                    <a href="javascript:;">
                        <span class="title">İletişim</span>
                        <span class="arrow"></span>
                    </a>
                    <span class="icon-thumbnail"><i class="pg-map"></i></span>
                    <ul class="sub-menu">
                        <li>
                            @Html.ActionLink("Ekle", "Index", "Contact")
                            <span class="icon-thumbnail"><i class="pg-plus"></i></span>
                        </li>
                        <li>
                            @Html.ActionLink("Listele", "Index", "ContactList")
                            <span class="icon-thumbnail"><i class="pg-menu"></i></span>
                        </li>
                    </ul>
                </li>


               
                
                <li class="">
                    <a href="javascript:;">
                        <span class="title">Yönetim</span>
                        <span class="arrow"></span>
                    </a>
                    <span class="icon-thumbnail"><i class="glyphicon glyphicon-star"></i></span>
                    <ul class="sub-menu">
                        <li>                            
                            @Html.ActionLink("Kişi Ekle", "Index", "BoardMembers")
                            <span class="icon-thumbnail"><i class="pg-plus"></i></span>
                        </li>
                        <li>
                            @Html.ActionLink("Listele", "Index", "BoardMembersList")
                            <span class="icon-thumbnail"><i class="pg-menu"></i></span>
                        </li>
                    </ul>
                </li>               
            </ul>
            <div class="clearfix"></div>
        </div>
    </nav>






    <div class="header ">
        <!-- START MOBILE SIDEBAR TOGGLE -->
        <a href="#" class="btn-link toggle-sidebar hidden-lg-up pg pg-menu" data-toggle="sidebar">
        </a>
        <!-- END MOBILE SIDEBAR TOGGLE -->
        <div class="">
            <div class="brand inline   ">
               
            </div>
            
        </div>
        <div class="d-flex align-items-center">
            <!-- START User Info-->
           
            <div class="dropdown pull-right hidden-md-down">
               
            </div>
            <!-- END User Info-->
           
        </div>
    </div>

    <div class="container-fluid container-fixed-lg footer">
        <div class="copyright sm-text-center">


            @RenderBody()
            <hr />
            <footer>
                <p class="text-center">&copy; @DateTime.Now.Year - Blueprint Digital Solutions</p>
            </footer>
        </div>
    </div>


    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    
    <script src="assets/plugins/jquery/jquery-1.11.1.min.js" type="text/javascript"></script>
    <script src="assets/plugins/modernizr.custom.js" type="text/javascript"></script>
    <script src="assets/plugins/jquery-ui/jquery-ui.min.js" type="text/javascript"></script>
    <script src="assets/plugins/tether/js/tether.min.js" type="text/javascript"></script>
    <script src="assets/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="assets/plugins/jquery/jquery-easy.js" type="text/javascript"></script>
    <script src="assets/plugins/jquery-unveil/jquery.unveil.min.js" type="text/javascript"></script>
    <script src="assets/plugins/jquery-ios-list/jquery.ioslist.min.js" type="text/javascript"></script>
    <script src="assets/plugins/jquery-actual/jquery.actual.min.js"></script>
    <script src="assets/plugins/jquery-scrollbar/jquery.scrollbar.min.js"></script>
    <script type="text/javascript" src="assets/plugins/select2/js/select2.full.min.js"></script>
    <script type="text/javascript" src="assets/plugins/classie/classie.js"></script>
    <script src="assets/plugins/switchery/js/switchery.min.js" type="text/javascript"></script>
    <!-- END VENDOR JS -->
    <!-- BEGIN CORE TEMPLATE JS -->
    <script src="pages/js/pages.min.js"></script>
    <!-- END CORE TEMPLATE JS -->
    <!-- BEGIN PAGE LEVEL JS -->
    <script src="assets/js/scripts.js" type="text/javascript"></script>
    <!-- END PAGE LEVEL JS -->
    <!--Summer Note-->
    <script src="dist/summernote.js" type="text/javascript"></script>
</body>
        
</html>
